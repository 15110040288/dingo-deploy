cluster:
    name: dingo
exchange:
    host: {{ inventory_hostname }}
    port: 8765
client:
    coordinatorExchangeSvrList: {{ dingo_coordinator_exchange_connection_list }}
security:
    cipher:
        keyPath: {{ dingo_home }}/conf/dingodb.jks
        keyPass: dingodb
        storePass: dingodb
        alias: dingodb
        issuer: dingo
    verify: true
